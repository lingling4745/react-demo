{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns":"usage"
      },
      "react"
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "core.js":2
      }
    ],
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    ["@babel/plugin-proposal-class-properties", { "loose" : true }],
    "react-hot-loader/babel"
  ]
}